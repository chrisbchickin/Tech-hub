<p>homepage</p>
<header>
  <nav>
    <a href="/dashboard">dashboard</a>
    <a href="/">homepage</a>
    {{#if logged_in}}
    <a href="/" class="logout">logout</a>
    {{else}}
    <a href="/login">login</a>
    {{/if}}
  </nav>
</header>
{{#each posts as |post| }}
<div>
  <div>
    <h2>
      <a href="/posts/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>Created by {{post.user.username}} on {{format_date post.createdAt}}</p>
  </div>
</div>  
{{/each}}

<script src="../js/logout.js"></script>